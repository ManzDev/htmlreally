{
    "name": "htmlreally",
    "version": "1.0.0",
    "description": "A HTML5 quiz to test your knowledge",
    "main": "src/index.html",
    "scripts": {
        "test": "npx eslint src/*",
        "dev": "parcel serve src/index.html --open",
        "watch": "parcel watch src/index.html",
        "build": "rm -rf build/* && parcel build src/index.html --no-source-maps -d build --global global --public-url /htmlreally",
        "clean:cache": "rm -rf .cache dist build",
        "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
        "deploy": "git subtree push --prefix build origin gh-pages"
    },
    "keywords": [
        "html",
        "html5",
        "test",
        "quiz"
    ],
    "author": "Manz",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.4",
        "@babel/preset-env": "^7.4.4",
        "@vue/component-compiler-utils": "^3.0.0",
        "autoprefixer": "^9.5.1",
        "cross-env": "^5.2.0",
        "cssnano": "^4.1.10",
        "eslint": "^5.16.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-node": "^9.0.1",
        "eslint-plugin-promise": "^4.1.1",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-plugin-vue": "^5.2.2",
        "npm-run-all": "^4.1.5",
        "postcss-preset-env": "^6.6.0",
        "stylelint-config-standard": "^18.3.0",
        "vue-hot-reload-api": "^2.3.3",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "prismjs": "^1.16.0",
        "tippy.js": "^4.3.1",
        "vue": "^2.6.10"
    }
}
