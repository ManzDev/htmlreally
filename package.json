{
  "name": "htmlreally",
  "version": "1.0.0",
  "description": "A HTML5 quiz to test your knowledge",
  "main": "src/index.html",
  "scripts": {
    "test": "npx eslint src/*",
    "dev": "parcel serve src/index.html",
    "watch": "parcel watch src/index.html",
    "build": "rm -rf build/* && parcel build src/index.html --no-source-maps -d build --global global --public-url /htmlreally",
    "clean:cache": "rm -rf .cache dist build",
    "clean:all": "rm -rf node_modules package-lock.json .cache dist build",
    "deploy": "gh-pages -d build"
  },
  "keywords": [
    "html",
    "html5",
    "test",
    "quiz"
  ],
  "author": "Manz",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@vue/component-compiler-utils": "^3.2.0",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "postcss-preset-env": "^6.7.0",
    "stylelint-config-standard": "^20.0.0",
    "vue-hot-reload-api": "^2.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "prismjs": "^1.23.0",
    "tippy.js": "^4.3.5",
    "vue": "^2.6.11"
  }
}
